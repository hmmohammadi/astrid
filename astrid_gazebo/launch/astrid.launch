<?xml version="1.0"?>
<launch>
  <arg name="world_name" default="astrid" />
  <arg name="gui" default="true" />
  <arg name="run_camera" default="false"/>

  <include file="$(find gazebo_ros)/launch/empty_world.launch">
    <!-- <arg name="world_name" value="$(find astrid_gazebo)/worlds/$(arg world_name).world"/> -->
    <arg name="gui" value="$(arg gui)"/>
    
  </include>

  <!-- urdf xml robot description loaded on the Parameter Server, converting the xacro into a proper urdf file-->
  <param name="robot_description" command="$(find xacro)/xacro '$(find astrid_description)/xacro/astrid.xacro'" />

  <arg name="x" default="0"/>
  <arg name="y" default="0"/>
  <arg name="z" default="10.5"/>
  <!-- push robot_description to factory and spawn robot in gazebo -->
  <node name="astrid_spawn" pkg="gazebo_ros" type="spawn_model" output="screen" args="-urdf -param robot_description -model astrid" />

  <!-- ros_control racecar launch file -->
  <!-- <include file="$(find racecar_control)/launch/racecar_control.launch" ns="/"/> -->

  <!-- Spawn the MUXs -->
  <!-- <arg name="racecar_version" default="racecar-v2" />
  <include file="$(find racecar)/launch/mux.launch" ns="vesc" /> -->

  <!-- Publish "better odom" topic that is normally generated by the particle filter -->
    <!-- <node name="better_odom" pkg="topic_tools" type="relay"
          args="/vesc/odom /pf/pose/odom" /> -->
</launch>